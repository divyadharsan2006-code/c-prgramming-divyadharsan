#include <stdio.h>

int main() {
    int N;
    
   
    if (scanf("%d", &N) != 1) return 0;

    int arr[N];

   
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int isPalindrome = 1; 


    for (int i = 0; i < N / 2; i++) {
        if (arr[i] != arr[N - 1 - i]) {
            isPalindrome = 0; 
            break;
        }
    }

  
    if (isPalindrome) {
        printf("YES\n");
    } else {
        printf("NO\n");
    }

    return 0;
}
