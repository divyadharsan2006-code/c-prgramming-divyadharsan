#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);
    
    int arr[1000];
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    
    int X;
    scanf("%d", &X);
    
    int count = 0;
    for(int i = 0; i < N; i++) {
        if(arr[i] == X) {
            count++;
        }
    }
    
    printf("%d\n", count);
    return 0;
}
