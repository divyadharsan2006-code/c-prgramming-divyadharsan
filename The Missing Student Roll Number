#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);
    
    long long expected_sum = (long long)N * (N + 1) / 2;
    long long actual_sum = 0;
    
    for(int i = 0; i < N - 1; i++) {
        int num;
        scanf("%d", &num);
        actual_sum += num;
    }
    
    int missing = expected_sum - actual_sum;
    printf("%d\n", missing);
    
    return 0;
}
