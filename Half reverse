#include <stdio.h>

int main() {
    int n, i;
    int arr[20];
    int temp;
    scanf("%d", &n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    int start = n / 2;
    int end = n - 1;
    while(start < end) {
        temp = arr[start];
        arr[start] = arr[end];
        arr[end] = temp;
        start++;
        end--;
    }
    for(i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
